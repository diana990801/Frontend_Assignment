<svg width="800" height="1200" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="sunsetGradient" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#FF6B6B;stop-opacity:1" />
      <stop offset="30%" style="stop-color:#FFA07A;stop-opacity:1" />
      <stop offset="60%" style="stop-color:#FFD700;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#FF8C00;stop-opacity:1" />
    </linearGradient>
  </defs>
  <rect width="800" height="1200" fill="url(#sunsetGradient)"/>
  <circle cx="650" cy="250" r="80" fill="#FF4500" opacity="0.7"/>
  
  <!-- Simple dog silhouette (different pose) -->
  <g transform="translate(400, 700)">
    <!-- Body -->
    <ellipse cx="0" cy="0" rx="130" ry="70" fill="#D2691E"/>
    <!-- Head (raised up) -->
    <circle cx="-70" cy="-80" r="65" fill="#D2691E"/>
    <!-- Ears (perked up) -->
    <ellipse cx="-100" cy="-120" rx="22" ry="45" fill="#8B4513" transform="rotate(-10 -100 -120)"/>
    <ellipse cx="-40" cy="-120" rx="22" ry="45" fill="#8B4513" transform="rotate(10 -40 -120)"/>
    <!-- Snout -->
    <ellipse cx="-95" cy="-60" rx="32" ry="28" fill="#CD853F"/>
    <!-- Nose -->
    <ellipse cx="-105" cy="-60" rx="13" ry="11" fill="#000"/>
    <!-- Eyes -->
    <circle cx="-85" cy="-90" r="9" fill="#000"/>
    <circle cx="-55" cy="-90" r="9" fill="#000"/>
    <!-- Legs (sitting position) -->
    <rect x="-70" y="40" width="28" height="90" rx="14" fill="#D2691E"/>
    <rect x="-20" y="40" width="28" height="90" rx="14" fill="#D2691E"/>
    <!-- Front legs (bent) -->
    <rect x="50" y="20" width="25" height="60" rx="12" fill="#D2691E"/>
    <rect x="90" y="20" width="25" height="60" rx="12" fill="#D2691E"/>
    <!-- Tail (curved) -->
    <path d="M 120 -10 Q 160 -40 140 -80" stroke="#D2691E" stroke-width="35" fill="none" stroke-linecap="round"/>
  </g>
  
  <text x="400" y="1050" font-family="Arial, sans-serif" font-size="24" fill="#fff" text-anchor="middle" opacity="0.8">
    Replace with actual dog sunset photo
  </text>
  <text x="400" y="1080" font-family="Arial, sans-serif" font-size="18" fill="#fff" text-anchor="middle" opacity="0.7">
    (dog-sunset.jpg - 800x1200px recommended)
  </text>
</svg>
