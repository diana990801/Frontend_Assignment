<svg width="800" height="1200" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="contactGradient" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#90EE90;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#98FB98;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#8FBC8F;stop-opacity:1" />
    </linearGradient>
  </defs>
  <rect width="800" height="1200" fill="url(#contactGradient)"/>
  
  <!-- Grass texture -->
  <rect x="0" y="900" width="800" height="300" fill="#228B22" opacity="0.3"/>
  
  <!-- Happy dog -->
  <g transform="translate(400, 600)">
    <!-- Body -->
    <ellipse cx="0" cy="0" rx="140" ry="85" fill="#CD853F"/>
    <!-- Head -->
    <circle cx="-90" cy="-50" r="70" fill="#CD853F"/>
    <!-- Ears (floppy) -->
    <ellipse cx="-125" cy="-60" rx="30" ry="50" fill="#A0522D" transform="rotate(-30 -125 -60)"/>
    <ellipse cx="-55" cy="-60" rx="30" ry="50" fill="#A0522D" transform="rotate(30 -55 -60)"/>
    <!-- Snout -->
    <ellipse cx="-110" cy="-30" rx="35" ry="30" fill="#DEB887"/>
    <!-- Nose -->
    <ellipse cx="-120" cy="-30" rx="14" ry="12" fill="#000"/>
    <!-- Eyes (happy) -->
    <circle cx="-100" cy="-60" r="10" fill="#000"/>
    <circle cx="-70" cy="-60" r="10" fill="#000"/>
    <!-- Smile -->
    <path d="M -130 -15 Q -110 -5 -90 -15" stroke="#000" stroke-width="3" fill="none" stroke-linecap="round"/>
    <!-- Tongue -->
    <ellipse cx="-110" cy="-8" rx="12" ry="8" fill="#FF69B4"/>
    <!-- Legs -->
    <rect x="-90" y="60" width="30" height="85" rx="15" fill="#CD853F"/>
    <rect x="-35" y="60" width="30" height="85" rx="15" fill="#CD853F"/>
    <rect x="20" y="60" width="30" height="85" rx="15" fill="#CD853F"/>
    <rect x="75" y="60" width="30" height="85" rx="15" fill="#CD853F"/>
    <!-- Tail (wagging) -->
    <ellipse cx="140" cy="-30" rx="70" ry="30" fill="#CD853F" transform="rotate(45 140 -30)"/>
    <!-- Spots -->
    <circle cx="-20" cy="-10" r="20" fill="#8B4513" opacity="0.6"/>
    <circle cx="40" cy="20" r="25" fill="#8B4513" opacity="0.6"/>
    <circle cx="-40" cy="-75" r="15" fill="#8B4513" opacity="0.6"/>
  </g>
  
  <text x="400" y="1050" font-family="Arial, sans-serif" font-size="24" fill="#333" text-anchor="middle" opacity="0.7">
    Replace with actual dog photo
  </text>
  <text x="400" y="1080" font-family="Arial, sans-serif" font-size="18" fill="#666" text-anchor="middle" opacity="0.6">
    (dog-contact.jpg - 800x1200px recommended)
  </text>
</svg>
